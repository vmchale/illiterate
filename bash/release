github-release upload -s $(cat ~/.git-token) -u vmchale -r illiterate -n lit-mips64el-linux-gnuabi64 -f target/lit-mips64el-linux-gnuabi64 -t "$(grep -P -o '\d+\.\d+\.\d+' src/cli.dats)"
